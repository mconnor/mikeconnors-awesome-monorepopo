{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./vite.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "${configDir}/",
    "paths": {
      "react": [
        "${configDir}/node_modules/preact/compat/"
      ],
      "react/jsx-runtime": [
        "${configDir}/node_modules/preact/jsx-runtime"
      ],
      "react-dom": [
        "${configDir}/node_modules/preact/compat/"
      ],
      "react-dom/*": [
        "${configDir}/node_modules/preact/compat/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}