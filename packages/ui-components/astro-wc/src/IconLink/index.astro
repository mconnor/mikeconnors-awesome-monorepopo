---
import type { ComponentProps } from 'astro/types';

import Icon from '#Icon/index.astro';
type IconProps = ComponentProps<typeof Icon>;


  interface Props extends IconProps {
href: string;
  
}






const {

  href,
  icon,
  class:
    className = 'fill-gray-900 stroke-gray-900 dark:fill-gray-50 dark:stroke-gray-50 h-10 w-10',

} = Astro.props;
---

<link-icon>
  <div>
    <a href={href}> <Icon class={className} icon={icon} /></a>
  </div>
</link-icon>

<script>
  class LinkIcon extends HTMLElement {
   
  }

  customElements.define('link-icon', LinkIcon);
</script>
