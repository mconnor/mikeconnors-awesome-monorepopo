---
interface Props {
  clips: string[];
  // class?: string;
}

const { clips } = Astro.props as Props;
---

<script src="./EasterEgg"></script>

<easter-egg data-clips={JSON.stringify(clips)}>
  <audio id="easter-audio" preload="none"></audio>

  <button id="secret-sound" class="btn place-self-center btn-primary"
    ><i class="ph ph-rabbit scale-200"></i></button
  >
</easter-egg>
