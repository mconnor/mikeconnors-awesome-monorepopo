---
import './Cluster.css';

import type {
  AlignItemsTypes,
  JustifyContentTypes,
  RmUnitType,
} from '#types/index.ts';

interface Props {
  justify?: JustifyContentTypes; //"flex-start"
  align?: AlignItemsTypes; // "flex-start"
  space?: RmUnitType;
  role?: 'list';
}

const props = Astro.props;
---

<script src="./Cluster.js"></script>

<cluster-l {...props}>
  <slot />
</cluster-l>
