{
  "public": true,
  "author": "Mike Connor",
  "name": "with-vite",
  "type": "module",
  "imports": {
    "#*": "./src/*",
    "#unit-types": "./src/UnitTypes.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mconnor/vite-turbo"
  },
  "keywords": [
    "vite",
    "eslint",
    "astro"
  ],
  "scripts": {
    "graph": "turbo run build --graph",
    "build": "turbo build",
    "preview": "turbo preview",
    "dev": "turbo --filter @app/multi dev",
    "lint": "turbo lint",
    "fix": "turbo fix",
    "inspect": "turbo inspect",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "check-types": "turbo check-types",
    "all": "turbo run lint test build",
    "update:all": "turbo run update --filter=..."
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">= 20.11.0 <21 || >= 21.2.0",
    "pnpm": ">=9.1.2"
  },
  "packageManager": "pnpm@9.7.0"
}
