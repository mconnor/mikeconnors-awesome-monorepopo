{
  "extends": ["//"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "!README.md", ".env*"],
      "outputs": ["dist/**"],
      "env": [
        "NODE_ENV",
        "VERCEL",
        "SENTRY_DSN",
        "SENTRY_AUTH_TOKEN",
        "ENABLE_EXPERIMENTAL_COREPACK",
        "DEBUG_MODE",
        "USER_NAME",
        "DOMAIN"
      ]
    }
  }
}
