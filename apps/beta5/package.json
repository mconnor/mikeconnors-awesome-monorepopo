{
  "name": "@app/beta5",
  "type": "module",
  "version": "0.0.1",
  "author": "Mike Connor",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "spell-check": "cspell --no-summary --no-progress --no-colors --config cspell.json \"**/*.{md,mts,ts,tsx}\"",
    "dev": "astro dev",
    "astro-check": "astro check",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier . --write",
    "lint": "eslint . --max-warnings 0",
    "lint:json": "eslint . -c jsonLint.config.mjs",
    "inspect:json": "eslint . -c jsonLint.config.mjs --inspect-config",
    "inspect": "eslint . --inspect-config",
    "fix": "eslint . --fix",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.0.0-beta.2",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "astro": "^5.0.0-beta.5",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/eslint-config-prettier": "^6.11.3",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "vitest": "^2.1.3"
  }
}